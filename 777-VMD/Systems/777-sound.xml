    <filter>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <condition>
                <property>/sim/current-view/internal</property>
            </condition>
            <expression>
                <product>
                    <property>/surface-positions/reverser-norm[0]</property>
                    <table>
                        <property>engines/engine[0]/rpm</property>
                        <entry><ind>2.0</ind>  <dep>0.000</dep></entry>
                        <entry><ind>13.2</ind> <dep>0.324</dep></entry>
                        <entry><ind>60.5</ind> <dep>0.924</dep></entry>
