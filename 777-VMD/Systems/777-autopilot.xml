<?xml version="1.0" encoding="UTF-8"?>

<!-- 777 Autopilot -->
<!-- Copyright (c) 2019 Joshua Davidson (Octal450) -->

<PropertyList>
	
	<!-- Roll Axis -->
	
	<filter>
		<name>TO/GA or ROLLOUT</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<or>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>TO/GA</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>ROLLOUT</value>
						</equals>
					</or>
				</and>
			</condition>
		</enable>
		<input>0</input>
		<output>/autopilot/internal/target-roll-deg</output>
	</filter>
	
	<filter>
		<name>Heading Input</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<equals>
					<property>/systems/navigation/hdgref/reference</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/orientation/heading-deg</property>
		</input>
		<input>/orientation/heading-magnetic-deg</input>
		<output>/autopilot/internal/heading</output>
	</filter>
	
	<filter>
		<name>Track Input</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<equals>
					<property>/systems/navigation/hdgref/reference</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/orientation/track-deg</property>
		</input>
		<input>/orientation/track-magnetic-deg</input>
		<output>/autopilot/internal/track</output>
	</filter>
	
	<filter>
		<name>HEADING ERROR DEG</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<condition>
				<or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG SEL</value>
					</equals>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG HOLD</value>
					</equals>
				</or>
			</condition>
			<property>/autopilot/settings/heading-bug-deg</property>
			<offset>
				<property>/autopilot/internal/heading</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<input>
			<condition>
				<or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK SEL</value>
					</equals>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK HOLD</value>
					</equals>
				</or>
			</condition>
			<property>/autopilot/settings/heading-bug-deg</property>
			<offset>
				<property>/autopilot/internal/track</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<input>
			<condition>
				<and>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG SEL</value>
					</not-equals>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG HOLD</value>
					</not-equals>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK SEL</value>
					</not-equals>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK HOLD</value>
					</not-equals>
				</and>
			</condition>
			<value>0</value>
		</input>
		<output>/autopilot/internal/heading-error-deg</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
	</filter>

	<filter>
		<name>IT-CONTROLLER: HDG ROLL</name>
		<type>gain</type>
		<gain>
			<expression>
				<table>
					<property>/velocities/airspeed-kt</property>
					<entry><ind>140</ind><dep>1.5</dep></entry>
					<entry><ind>360</ind><dep>2.5</dep></entry>
				</table>
			</expression>
		</gain>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<or>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>HDG SEL</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>HDG HOLD</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>TRK SEL</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>TRK HOLD</value>
						</equals>
					</or>
				</and>
			</condition>
		</enable>
		<input>/autopilot/internal/heading-error-deg</input>
		<reference>0</reference>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>/instrumentation/afds/settings/bank-min</min>
		<max>/instrumentation/afds/settings/bank-max</max>
	</filter>

	<filter>
		<name>IT-CONTROLLER: LNAV ROLL</name>
		<type>gain</type>
		<gain>1.75</gain>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>LNAV</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>/autopilot/internal/course-error-deg</input>
		<reference>0</reference>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>/instrumentation/afds/settings/bank-min</min>
		<max>/instrumentation/afds/settings/bank-max</max>
	</filter>

	<filter>
		<name>nav1 heading error computer/normalizer</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>
			<property>/instrumentation/nav[0]/radials/target-auto-hdg-deg</property>
			<offset>
				<property>/orientation/heading-deg</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<output>/autopilot/internal/nav1-heading-error-deg</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
	</filter>

	<filter>
		<name>IT-CONTROLLER: VORLOC ROLL</name>
		<type>gain</type>
		<gain>2.2</gain>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>LOC</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>/autopilot/internal/nav1-heading-error-deg</input>
		<reference>0</reference>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>/instrumentation/afds/settings/bank-min</min>
		<max>/instrumentation/afds/settings/bank-max</max>
	</filter>
	
	<filter>
		<name>ROLL DEG SYNC</name>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<and>
					<not><property>/instrumentation/afds/inputs/AP</property></not>
					<not><property>/instrumentation/afds/inputs/FD</property></not>
				</and>
			</condition>
		</enable>
		<input>/orientation/roll-deg</input>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>/instrumentation/afds/settings/bank-min</min>
		<max>/instrumentation/afds/settings/bank-max</max>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: TARGET ROLL CMD</name>
		<input>/autopilot/internal/target-roll-deg</input>
		<output>/autopilot/internal/target-roll</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<condition>
				<and>
					<not><property>/instrumentation/afds/inputs/AP</property></not>
					<not><property>/instrumentation/afds/inputs/FD</property></not>
				</and>
			</condition>
			<value>50</value>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<not><property>/instrumentation/afds/inputs/AP</property></not>
			</condition>
			<value>15</value>
		</max-rate-of-change>
		<max-rate-of-change>6</max-rate-of-change>
	</filter>
	
	<filter>
		<name>System Command: Roll Difference</name>
		<type>gain</type>
		<gain>-0.84</gain>
		<enable>
			<condition>
				<property>/instrumentation/afds/inputs/AP</property>
			</condition>
		</enable>
		<input>/orientation/roll-deg</input>
		<reference>/autopilot/internal/target-roll</reference>
		<output>/autopilot/internal/roll-rate</output>
		<min>-4.8</min>
		<max>4.8</max>
	</filter>
	
	<pid-controller>
		<name>System Command: Roll Rate</name>
		<enable>
			<condition>
				<and>
					<property>/instrumentation/afds/inputs/AP</property>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>ROLLOUT</value>
					</not-equals>
					<or>
						<equals>
							<property>/gear/gear[1]/wow</property>
							<value>0</value>
						</equals>
						<equals>
							<property>/gear/gear[2]/wow</property>
							<value>0</value>
						</equals>
					</or>
				</and>
			</condition>
		</enable>
		<input>/orientation/roll-rate-degps</input>
		<reference>/autopilot/internal/roll-rate</reference>
		<output>/autopilot/internal/aileron-pid</output>
		<config>
			<Kp>
				<expression>
					<product>
						<table>
							<property>/velocities/airspeed-kt</property>
							<entry><ind>140</ind><dep>0.12</dep></entry>
							<entry><ind>360</ind><dep>0.04</dep></entry>
						</table>
						<table> <!-- Increase deflection if outer ailerons are locked -->
							<property>/fcs/logic/outer-aileron-locked</property>
							<entry><ind>0</ind><dep>1.0</dep></entry>
							<entry><ind>1</ind><dep>1.4</dep></entry>
						</table>
					</product>
				</expression>
			</Kp>
			<Ti>1.0</Ti>
			<Td>0.0001</Td>
			<u_min>-1.0</u_min>
			<u_max>1.0</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>IT-CONTROLLER: AILERON ROLLOUT</name>
		<type>gain</type>
		<gain>-0.1</gain>
		<enable>
			<condition>
				<property>/instrumentation/afds/inputs/AP</property>
			</condition>
		</enable>
		<input>
			<expression>
				<table>
					<property>/orientation/roll-deg</property>
					<entry><ind>-30</ind><dep>-30</dep></entry>
					<entry><ind> -1</ind><dep>  0</dep></entry>
					<entry><ind>  0</ind><dep>  0</dep></entry>
					<entry><ind>  1</ind><dep>  0</dep></entry>
					<entry><ind> 30</ind><dep> 30</dep></entry>
				</table>
			</expression>
		</input>
		<reference>0</reference>
		<output>/autopilot/internal/aileron-rollout</output>
	</filter>
	
	<filter>
		<name>AILERON SYNC</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1.0</gain>
		<enable>
			<condition>
				<not><property>/instrumentation/afds/inputs/AP</property></not>
			</condition>
		</enable>
		<input>/controls/flight/aileron</input>
		<output>/fcs/fbw/ap/roll-cmd</output>
	</filter>
	
	<filter>
		<name>System Command: Aileron</name>
		<type>noise-spike</type>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<property>/instrumentation/afds/inputs/AP</property>
			</condition>
		</enable>
		<input>
			<condition>
				<and>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<property>/autopilot/internal/aileron-pid</property>
		</input>
		<input>/autopilot/internal/aileron-rollout</input>
		<output>/fcs/fbw/ap/roll-cmd</output>
		<max-rate-of-change>1.2</max-rate-of-change>
	</filter>
	
	<!-- Pitch Axis -->
	
	<filter>
		<name>ALTITUDE CAPTURE/HOLD</name>
		<type>gain</type>
		<gain>-5</gain>
		<input>
			<expression>
				<sum>
					<property>/instrumentation/altimeter/indicated-altitude-ft</property>
					<value>-2</value>
				</sum>
			</expression>
		</input>
		<reference>/autopilot/settings/actual-target-altitude-ft</reference>
		<output>/autopilot/internal/target-fpm-alt</output>
		<min>-2000</min>
		<max>2000</max>
	</filter>
	
	<predict-simple>
		<name>IAS 15 SECOND PREDICTOR</name>
		<input>instrumentation/airspeed-indicator/indicated-speed-kt</input>
		<output>/autopilot/internal/lookahead-15-sec-airspeed-kt</output>
		<seconds>15.0</seconds>
		<filter-gain>0.0</filter-gain>
	</predict-simple>
	
	<predict-simple>
		<name>MACH 15 SECOND PREDICTOR</name>
		<input>instrumentation/airspeed-indicator/indicated-mach</input>
		<output>/autopilot/internal/lookahead-15-sec-mach</output>
		<seconds>15.0</seconds>
		<filter-gain>0.0</filter-gain>
	</predict-simple>
	
	<filter>
		<name>KTS INPUT FILTER</name>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>instrumentation/afds/ias-target-kt</input>
		<output>/autopilot/internal/flch-kts</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<condition>
				<or>
					<and>
						<not-equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>TO/GA</value>
						</not-equals>
						<not-equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>FLCH SPD</value>
						</not-equals>
					</and>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>1</value>
					</equals>
				</or>
			</condition>
			<value>100</value>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<and>
					<or>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>TO/GA</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>FLCH SPD</value>
						</equals>
					</or>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<value>5</value>
		</max-rate-of-change>
	</filter>
	
	<filter>
		<name>MACH INPUT FILTER</name>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>autopilot/settings/target-speed-mach</input>
		<output>/autopilot/internal/flch-mach</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<condition>
				<or>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>FLCH SPD</value>
					</not-equals>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>1</value>
					</equals>
				</or>
			</condition>
			<value>1</value>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<and>
					<equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>FLCH SPD</value>
					</equals>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<value>0.05</value>
		</max-rate-of-change>
	</filter>
	
	<filter>
		<name>INTERNAL VERTICAL SPEED COMPUTER</name>
		<type>gain</type>
		<gain>1.0</gain>
		<input>/instrumentation/gps/indicated-vertical-speed</input>
		<output>/autopilot/internal/vert-speed-fpm</output>
	</filter>
	
	<pid-controller>
		<name>FLCH SPEED BY PITCH</name>
		<enable>
			<condition>
				<or>
					<equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>TO/GA</value>
					</equals>
					<equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>FLCH SPD</value>
					</equals>
					<equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>VNAV SPD</value>
					</equals>
					<and>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>VNAV PTH</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/inputs/vnav-path-mode</property>
							<value>2</value>
						</equals>
					</and>
				</or>
			</condition>
		</enable>
		<input>
			<condition>
				<equals>
					<property>/instrumentation/afds/inputs/ias-mach-selected</property>
					<value>0</value>
				</equals>
			</condition>
			<property>/autopilot/internal/lookahead-15-sec-airspeed-kt</property>
		</input>
		<input>
			<condition>
				<equals>
					<property>/instrumentation/afds/inputs/ias-mach-selected</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/autopilot/internal/lookahead-15-sec-mach</property>
			<scale>1000.0</scale>
		</input>
		<reference>
			<condition>
				<equals>
					<property>/instrumentation/afds/ap-modes/pitch-mode</property>
					<value>TO/GA</value>
				</equals>
			</condition>
			<expression>
				<sum>
					<property>/instrumentation/weu/state/v2</property>
					<value>10</value>
				</sum>
			</expression>
		</reference>
		<reference>
			<condition>
				<and>
					<equals>
						<property>/instrumentation/afds/inputs/ias-mach-selected</property>
						<value>0</value>
					</equals>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>TO/GA</value>
					</not-equals>
				</and>
			</condition>
			<property>/autopilot/internal/flch-kts</property>
		</reference>
		<reference>
			<condition>
				<equals>
					<property>/instrumentation/afds/inputs/ias-mach-selected</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/autopilot/internal/flch-mach</property>
			<scale>1000.0</scale>
		</reference>
		<output>/autopilot/internal/target-fpm-spd</output>
		<config>
			<Kp>
				<condition>
					<or>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>FLCH SPD</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>VNAV SPD</value>
						</equals>
						<and>
							<equals>
								<property>/instrumentation/afds/ap-modes/pitch-mode</property>
								<value>VNAV PTH</value>
							</equals>
							<equals>
								<property>/instrumentation/afds/inputs/vnav-path-mode</property>
								<value>2</value>
							</equals>
						</and>
					</or>
				</condition>
				<value>-55</value>
			</Kp>
			<Kp>
				<condition>
					<equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>TO/GA</value>
					</equals>
				</condition>
				<value>-60</value>
			</Kp>
			<Ti>2.5</Ti>        
			<Td>0.001</Td>
			<u_min>
				<condition>
					<greater-than-equals>
						<property>/autopilot/settings/actual-target-altitude-ft</property>
