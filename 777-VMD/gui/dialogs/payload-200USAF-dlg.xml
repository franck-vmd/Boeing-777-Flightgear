        <halign>left</halign>
        <live>true</live>
    </checkbox>
    
    <checkbox>
        <row>4</row>
        <col>1</col>
        <pref-width>120</pref-width>
        <pref-height>20</pref-height>
        <label>Catering Door 1R</label>
        <property>services/catering/enable</property>
        <binding>
            <command>dialog-apply</command>
        </binding>
        <halign>left</halign>
        <live>true</live>
    </checkbox>
    
    <checkbox>
        <row>4</row>
        <col>2</col>
        <pref-width>120</pref-width>
        <pref-height>20</pref-height>
        <label>Connect  1R</label>
        <property>services/catering/move</property>
        <binding>
            <command>dialog-apply</command>
        </binding>
        <halign>left</halign>
        <live>true</live>
    </checkbox>

</group>

<hrule />

<group>
<layout>table</layout>
    
    <button>
        <row>0</row>
        <col>0</col>
        <legend>Start loading</legend>
        <pref-width>120</pref-width>
        <pref-height>24</pref-height>
        <binding>
            <command>nasal</command>
                <script>
                    if ((getprop("services/payload/bagage2") == 1) and (getprop("services/payload/baggage-truck1-enable") == 1) and (getprop("services/payload/baggage-truck2-enable") == 1) and (getprop("services/payload/cargo_service") == 1) and (getprop("services/catering/enable") == 1)) {
                        setprop("services/payload/baggage-loading", 1);
                        var loadingtime = math.round((getprop("services/payload/belly-request-lbs") - getprop("services/payload/belly-onboard-lbs")) / (150 * (getprop("services/payload/baggage-truck1-enable") + getprop("services/payload/baggage-truck2-enable"))) / 60 * getprop("services/payload/speed"));
                        screen.log.write("Baggage loading started. Estimated loading time: " ~ loadingtime ~ " minutes.", 0, 0.584, 1);
                        setprop("services/payload/loadingtime_remaining", "Est. " ~ loadingtime ~ " min remaining");
                    } else {
                        screen.log.write("Please enable a baggage truck first!", 1, 0, 0);
                    }
                </script>
        </binding>
    </button>
    
    <button>
        <row>0</row>
        <col>1</col>
        <legend>Normal</legend>
        <pref-width>120</pref-width>
        <pref-height>24</pref-height>
        <binding>
            <command>property-assign</command>
            <property>services/payload/speed</property>
            <value>6.0</value>
        </binding>
        <binding>
            <command>property-assign</command>
            <property>services/payload/speed-text</property>
            <value>"Normal"</value>
        </binding>
    </button>

    <button>
        <row>0</row>
        <col>2</col>
        <halign>left</halign>
        <legend>catering1</legend>
        <pref-width>120</pref-width>
        <pref-height>24</pref-height>
        <binding>
            <command>nasal</command>
                <script>
                    <![CDATA[
                    if (((getprop("services/catering/enable") != 0)) and (getprop("services/payload/catering1-request-lbs") != 0)) {
                        if (getprop("services/payload/catering1-max-lbs") < getprop("services/payload/catering1-request-lbs")) {
                            screen.log.write("Captain, we can't load so many catering trolleys, please reduce the requested catering weight", 1, 0, 0);
                        } else {
                            setprop("services/payload/catering1-loading", 1);
                            screen.log.write("Catering loading started.", 0, 0.584, 1);
                        }
                    } elsif (getprop("services/payload/catering1-request-lbs") != 0) {
                        screen.log.write("Please enable at least one catering truck first!", 1, 0, 0);
                    }
                    ]]>
                </script>
        </binding>
    </button>
    
    <button>
        <row>1</row>
        <col>0</col>
        <legend>Start unloading</legend>
        <pref-width>120</pref-width>
        <pref-height>24</pref-height>
        <binding>
            <command>nasal</command>
                <script>
                    if ((getprop("services/payload/bagage2") == 1) and (getprop("services/payload/baggage-truck1-enable") == 1) and (getprop("services/payload/baggage-truck2-enable") == 1) and (getprop("services/payload/cargo_service") == 1) and (getprop("services/catering/enable") == 1)) {
                        setprop("services/payload/baggage-loading", 2);
                        var loadingtime = math.round(getprop("services/payload/belly-onboard-lbs") / (150 * (getprop("services/payload/baggage-truck1-enable") + getprop("services/payload/baggage-truck2-enable"))) / 60 * getprop("services/payload/speed"));
                        screen.log.write("Baggage unloading started. Estimated unloading time: " ~ loadingtime ~ " minutes.", 0, 0.584, 1);
                    } else {
                        screen.log.write("Please enable a baggage truck first!", 1, 0, 0);
                    }
                </script>
        </binding>
    </button>
    
    <button>
        <row>1</row>
        <col>1</col>
        <legend>Fast</legend>
        <pref-width>120</pref-width>
        <pref-height>24</pref-height>
        <binding>
            <command>property-assign</command>
            <property>services/payload/speed</property>
            <value>0.1</value>
        </binding>
        <binding>
            <command>property-assign</command>
            <property>services/payload/speed-text</property>
            <value>"Fast"</value>
        </binding>
    </button>

    <button>
        <row>1</row>
        <col>2</col>
        <halign>left</halign>
        <legend>uncatering</legend>
        <pref-width>120</pref-width>
        <pref-height>24</pref-height>
        <binding>
            <command>nasal</command>
                <script>
                    <![CDATA[
                    if (((getprop("services/catering/enable") != 0)) and (getprop("services/payload/catering1-onboard-lbs") != 0)) {
                        setprop("services/payload/catering1-loading", 2);
                        screen.log.write("Catering and waste unloading started.", 0, 0.584, 1);
                    } elsif (getprop("services/payload/catering1-onboard-lbs") != 0) {
                        screen.log.write("Please enable a catering truck to the galleys that contain trolleys first!", 1, 0, 0);
                    }
                    ]]>
                </script>
        </binding>
    </button>
    
    <button>
        <legend>Stop (un)loading</legend>
        <row>2</row>
        <col>0</col>
        <pref-width>120</pref-width>
        <pref-height>24</pref-height>
        <binding>
            <command>property-assign</command>
            <property>services/payload/baggage-loading</property>
            <value>0</value>
        </binding>
    </button>
    
    <button>
        <row>2</row>
        <col>1</col>
        <legend>Instant</legend>
        <pref-width>120</pref-width>
        <pref-height>24</pref-height>
        <binding>
            <command>property-assign</command>
            <property>services/payload/speed-text</property>
            <value>"Instant"</value>
        </binding>
        <binding>
            <command>property-assign</command>
            <property>services/payload/speed</property>
            <value>0</value>
        </binding>
    </button>

    <button>
        <legend>stop</legend>
        <pref-width>120</pref-width>
        <pref-height>24</pref-height>
        <row>2</row>
        <col>2</col>
        <halign>left</halign>
        <binding>
            <command>nasal</command>
            <script>
                if ((getprop("services/payload/catering1-loading") == 1) or (getprop("services/payload/catering1-loading") == 3)) {
                    setprop("services/payload/catering1-loading", 0);
                    screen.log.write("Catering loading stopped.", 0, 0.584, 1);
                } elsif ((getprop("services/payload/catering1-loading") == 2) or (getprop("services/payload/catering1-loading") == 4)) {
                    setprop("services/payload/catering1-loading", 0);
                    screen.log.write("Catering unloading stopped.", 0, 0.584, 1);
                }
                setprop("services/payload/loadingtime_remaining", " ");
            </script>
        </binding>
    </button>

    <text>
        <row>3</row>
        <col>0</col>
        <halign>center</halign>
        <property>services/payload/loadingtime_remaining</property>
        <live>true</live>
        <pref-width>110</pref-width>
        <pref-height>18</pref-height>
    </text>
    
    <text>
        <row>3</row>
        <col>1</col>
        <halign>right</halign>
        <property>services/payload/speed-text</property>
        <live>true</live>
        <pref-width>110</pref-width>
        <pref-height>18</pref-height>
    </text>
    
</group>

<hrule />

</PropertyList>
