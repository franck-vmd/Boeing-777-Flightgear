<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
  <name>Groun Ops</name>
  <layout>vbox</layout>
  <group>
    <layout>hbox</layout>
    <text>
      <label>Ground Operations X8</label>
    </text>
		<button>
                        <pref-width>16</pref-width>
			<pref-height>16</pref-height>
			<legend>x</legend>
			<default>1</default>
			<keynum>27</keynum>
			<border>2</border>
			<binding>
				<command>dialog-close</command>
      </binding>
		</button>
  </group>
  <hrule/>

  <group>
    <layout>vbox</layout>
    <halign>center</halign>

   <checkbox>
      <halign>left</halign>
      <label>Car</label>
      <property>controls/switches/car_service</property>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/car_service</property>
      </binding>
    </checkbox>

   <checkbox>
      <halign>left</halign>
      <label>flight_attendants</label>
      <property>controls/switches/Flight</property>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/Flight</property>
      </binding>
    </checkbox>
    
   <checkbox>
      <halign>left</halign>
      <label>mecanicien</label>
      <property>controls/switches/Mecanicien</property>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/Mecanicien</property>
      </binding>
    </checkbox>
    
    <checkbox>
      <halign>left</halign>
      <label>mecanicien1</label>
      <property>controls/switches/Mecanicien1</property>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/Mecanicien1</property>
      </binding>
    </checkbox>

   <checkbox>
      <halign>left</halign>
      <label>Engine Protection</label>
      <property>controls/switches/protection</property>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/protection</property>
      </binding>
    </checkbox>

   <checkbox>
      <halign>left</halign>
      <label>passagers</label>
      <property>controls/switches/passagers</property>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/passagers</property>
      </binding>
    </checkbox>

  </group>
  <hrule/>

</PropertyList>
