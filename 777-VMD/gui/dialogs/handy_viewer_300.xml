<?xml version="1.0"?>
<PropertyList>

<name>handyviewer300</name>
    <layout>vbox</layout>
    <padding>20</padding>

<group>
  <layout>hbox</layout>
  <text>
     <label>Handy Viewer 300</label>
  </text>
  <empty><stretch>true</stretch></empty>

  <button>
      <legend>x</legend>
      <key>Esc</key>
      <pref-width>16</pref-width>
      <pref-height>16</pref-height>
      <border>2</border>
      <binding>
        <command>dialog-close</command>
      </binding>
    </button>

 </group>

