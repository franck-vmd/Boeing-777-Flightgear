    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>EFIS_Lettering</object-name>
        <object-name>EFIS_Lettering_Btns</object-name>
    </effect>

    <animation>
        <type>material</type>
        <object-name>EFIS_Lettering</object-name>
        <object-name>EFIS_Lettering_Btns</object-name>
        <condition>
            <greater-than>
                <property>/sim/time/sun-angle-rad</property>
                <value>1.70</value>
            </greater-than>
             <greater-than>
                <property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
             </greater-than>
        </condition>
        <emission>
            <red>0.2</red>
            <green>0.8</green>
            <blue>0.2</blue>
            <factor-prop>controls/lighting/panel-flood-intencity</factor-prop>
        </emission>
    </animation>

