                <property>sim/multiplay/generic/int[2]</property>
        <equals>
            <property>gear/gear[0]/position-norm</property>
            <value>1.0</value>
        </equals>
        </and>
        <not>
          <property>controls/lighting/landing-light[0]</property>
        </not>
    </condition>
    </animation>
    
