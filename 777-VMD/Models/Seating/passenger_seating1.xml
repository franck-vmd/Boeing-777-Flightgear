<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>Sieges777_200.ac</path>
  
  <animation>
		<type>material</type>
         <object-name>Sieges777200</object-name>
         <object-name>Sieges777200lumiere</object-name>
         <object-name>Sieges777200tv</object-name>
		<texture>texture_sieges2.png</texture>
	</animation>

        <animation>
        <type>material</type>
        <object-name>Sieges777200</object-name>
        <object-name>Sieges777200lumiere</object-name>
        <condition>
         <and>
		<greater-than>
			<property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
		</greater-than>
        <greater-than>
			<property>systems/electrical/outputs/cabin-lights</property>
                <value>1.0</value>
		</greater-than>
         </and>
        </condition>
        <emission>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </emission>
    </animation>

    <animation>
    <type>select</type>
    <object-name>Sieges777200tv</object-name>
    <condition>
         <or>
         <less-than>
                <property>systems/electrical/outputs/efis</property>
                <value>15</value>
            </less-than>
        <less-than>
			<property>systems/electrical/outputs/cabin-lights</property>
                <value>1.0</value>
		</less-than>
        </or>
        </condition>
  </animation>
               
               <animation>
        <type>material</type>
        <object-name>Sieges777200lumiere</object-name>
        <condition>
        <and>
             <property>controls/electric/battery-switch</property>
             <property>aaa/oh/PB59/IND1.toggle</property>
        </and>
        </condition>
        <emission>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.0</blue>
        </emission>
    </animation>
 
</PropertyList>
