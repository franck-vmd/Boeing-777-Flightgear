               <object-name>siegeB8</object-name>
        <condition>
         <and>
		<greater-than>
			<property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
		</greater-than>
        <greater-than>
			<property>systems/electrical/outputs/cabin-lights</property>
                <value>1.0</value>
		</greater-than>
         </and>
        </condition>
        <emission>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </emission>
    </animation>
          
