<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>tv777300.ac</path>
  
  <animation>
		<type>material</type>
        <object-name>TV777</object-name>
        <object-name>TV7772</object-name>
        <object-name>TV7773</object-name>
		<texture>chambreensemble.png</texture>
	</animation>

    <animation>
    <type>select</type>
    <object-name>TV7772</object-name>
    <condition>
        <and>
        <greater-than>
                <property>systems/electrical/outputs/efis</property>
                <value>15</value>
            </greater-than>
        <greater-than>
			<property>systems/electrical/outputs/cabin-lights</property>
                <value>1.0</value>
		</greater-than>
        </and>
        </condition>
  </animation>

   <animation>
    <type>select</type>
    <object-name>TV777</object-name>
    <condition>
         <or>
         <less-than>
                <property>systems/electrical/outputs/efis</property>
                <value>15</value>
            </less-than>
        <less-than>
			<property>systems/electrical/outputs/cabin-lights</property>
                <value>1.0</value>
		</less-than>
        </or>
        </condition>
  </animation>

     <animation>
        <type>material</type>
        <object-name>TV7773</object-name>
        <object-name>TV7772</object-name>
        <condition>
         <and>
		<greater-than>
			<property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
		</greater-than>
        <greater-than>
			<property>systems/electrical/outputs/cabin-lights</property>
                <value>1.0</value>
		</greater-than>
         </and>
        </condition>
        <emission>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </emission>
    </animation>  
 
</PropertyList>
