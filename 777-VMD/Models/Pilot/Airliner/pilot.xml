        <effect>
         <name>777-interior</name>
        <inherits-from>Aircraft/777-VMD/Models/Effects/777-interior</inherits-from>
                         <object-name>Head</object-name>
                         <object-name>LeftUpperArm</object-name>
		         <object-name>LeftLowerArm</object-name>
		         <object-name>LeftHand</object-name>
		         <object-name>RightUpperArm</object-name>
		         <object-name>RightLowerArm</object-name>
		         <object-name>RightHand</object-name>
                         <object-name>RightFoot</object-name>
                         <object-name>LeftFoot</object-name>
                         <object-name>LeftLowerLeg</object-name>
		         <object-name>LeftUpperLeg</object-name>
                         <object-name>RightLowerLeg</object-name>
                         <object-name>RightUpperLeg</object-name>
                         <object-name>Body</object-name>
               </effect>

         <animation>
        <type>material</type>
                         <object-name>Head</object-name>
                         <object-name>LeftUpperArm</object-name>
		         <object-name>LeftLowerArm</object-name>
		         <object-name>LeftHand</object-name>
		         <object-name>RightUpperArm</object-name>
		         <object-name>RightLowerArm</object-name>
		         <object-name>RightHand</object-name>
                         <object-name>RightFoot</object-name>
                         <object-name>LeftFoot</object-name>
                         <object-name>LeftLowerLeg</object-name>
		         <object-name>LeftUpperLeg</object-name>
                         <object-name>RightLowerLeg</object-name>
                         <object-name>RightUpperLeg</object-name>
                         <object-name>Body</object-name>
        <condition>
                <property>controls/lighting/cockpit</property>
        </condition>
        <emission>
            <red>0.25</red>
            <green>0.25</green>
            <blue>0.25</blue>
            <factor-prop>controls/lighting/dome-intencity</factor-prop>
        </emission>
    </animation>

         <!-- Siege -->

             <animation>
		<type>translate</type>
    <object-name>Body</object-name>
    <object-name>LeftFoot</object-name>
    <object-name>LeftLowerLeg</object-name>
    <object-name>LeftUpperLeg</object-name>
    <object-name>RightFoot</object-name>
    <object-name>RightLowerLeg</object-name>
    <object-name>RightUpperLeg</object-name>
		<property>sim/multiplay/generic/float[58]</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>0.75</ind><dep>0</dep></entry>
			<entry><ind>0.80</ind><dep>0.01</dep></entry>
			<entry><ind>0.85</ind><dep>0.02</dep></entry>
			<entry><ind>1</ind><dep>0.20</dep></entry>
		</interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
	</animation>

        <animation>
		<type>translate</type>
    <object-name>Body</object-name>
    <object-name>LeftFoot</object-name>
    <object-name>LeftLowerLeg</object-name>
    <object-name>LeftUpperLeg</object-name>
    <object-name>RightFoot</object-name>
    <object-name>RightLowerLeg</object-name>
    <object-name>RightUpperLeg</object-name>
		<property>sim/multiplay/generic/float[58]</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>0.75</ind><dep>0.20</dep></entry>
			<entry><ind>0.80</ind><dep>0.22</dep></entry>
			<entry><ind>0.85</ind><dep>0.23</dep></entry>
			<entry><ind>1</ind><dep>0.24</dep></entry>
		</interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
	</animation>

       <animation>
		<type>rotate</type>
    <object-name>LeftFoot</object-name>
    <object-name>LeftLowerLeg</object-name>
    <object-name>RightFoot</object-name>
    <object-name>RightLowerLeg</object-name>
		<property>sim/multiplay/generic/float[58]</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>0.75</ind><dep>0</dep></entry>
			<entry><ind>0.80</ind><dep>0</dep></entry>
			<entry><ind>0.85</ind><dep>0</dep></entry>
			<entry><ind>1</ind><dep>50</dep></entry>
		</interpolation>
                 <center>
			<x-m>-1.53034</x-m>
			<y-m>-0.537591</y-m>
			<z-m>0.118202</z-m>
		 </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
	</animation>

