<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<path>pilot.ac</path>

	<params>
		<texture type="string">airliner.png</texture>
		<head>
			<roll-animation>
				<property type="string">fcs/fbw/yoke-aileron</property>
				<factor type="float">-10</factor>
				<offset-deg type="float">0</offset-deg>
				<min type="float">-10</min>
				<max type="float">10</max>
			</roll-animation>
			<pitch-animation>
				<property type="string">fcs/fbw/yoke-elevator</property>
				<factor type="float">15</factor>
				<offset-deg type="float">0</offset-deg>
				<min type="float">-15</min>
				<max type="float">15</max>
			</pitch-animation>
			<yaw-animation>
				<property type="string">controls/flight/rudder</property>
				<factor type="float">-60</factor>
				<offset-deg type="float">0</offset-deg>
				<min type="float">-60</min>
				<max type="float">60</max>
			</yaw-animation>
		</head>
		<arms>
			<upper-animation>
				<property type="string">fcs/fbw/yoke-aileron</property>
				<factor type="float">-20</factor>
				<offset-deg type="float">0</offset-deg>
				<min type="float">-20</min>
				<max type="float">20</max>
			</upper-animation>
			<lower-animation>
				<property type="string">fcs/fbw/yoke-elevator</property>
				<factor type="float">30</factor>
				<offset-deg type="float">0</offset-deg>
				<min type="float">-30</min>
				<max type="float">30</max>
			</lower-animation>
                        <lower1-animation>
				<property type="string">fcs/fbw/yoke-elevator</property>
				<factor type="float">-18</factor>
				<offset-deg type="float">0</offset-deg>
				<min type="float">-18</min>
				<max type="float">18</max>
			</lower1-animation>
		</arms>
		<legs>
			<upper-animation>
				<property type="string">controls/flight/rudder</property>
				<factor type="float">15</factor>
				<offset-deg type="float">0</offset-deg>
				<min type="float">-15</min>
				<max type="float">15</max>
			</upper-animation>
			<lower-animation>
				<property type="string">controls/flight/rudder</property>
				<factor type="float">15</factor>
				<offset-deg type="float">0</offset-deg>
				<min type="float">-15</min>
				<max type="float">15</max>
			</lower-animation>
		</legs>
                <body>
                        <body-animation>
				<property type="string">fcs/fbw/yoke-elevator</property>
				<factor type="float">15</factor>
				<offset-deg type="float">0</offset-deg>
				<min type="float">-15</min>
				<max type="float">15</max>
			</body-animation>
		</body>
                <body1>
                        <body1-animation>
				<property type="string">fcs/fbw/yoke-aileron</property>
				<factor type="float">8</factor>
				<offset-deg type="float">0</offset-deg>
				<min type="float">-8</min>
				<max type="float">8</max>
			</body1-animation>
		</body1>
