<PropertyList>

 <!-- Keyboard commands -->

 <key n="119">
  <name>w</name>
  <desc>Walk view: Walk forward.</desc>
  <binding>
   <command>nasal</command>
   <script>
     if (!walkview.forward(1.0)) {
     }
   </script>
  </binding>
  <mod-up>
   <binding>
    <command>nasal</command>
    <script>
      walkview.forward(0);
    </script>
   </binding>
  </mod-up>
 </key>

</PropertyList>
