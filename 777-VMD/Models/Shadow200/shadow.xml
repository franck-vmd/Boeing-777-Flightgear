<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>shadow.ac</path>
    
		<!-- No preview for fgrun-->
		<animation>
			<object-name>shadow</object-name>
			<object-name>shadow1</object-name>
			<nopreview/>
		</animation>
		
		<effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>shadow</object-name>
	<object-name>shadow1</object-name>
    </effect>
    
    <animation>
    <type>rotate</type>
    <name>c35</name>
    <object-name>shadow</object-name>
    <object-name>shadow1</object-name>
    <property>sim/multiplay/generic/float[57]</property>
    <interpolation>
      <entry><ind>1.000000</ind><dep>0.800000</dep></entry>
      <entry><ind>0.950000</ind><dep>0.700000</dep></entry>
      <entry><ind>0.900000</ind><dep>0.600000</dep></entry>
      <entry><ind>0.800000</ind><dep>0.500000</dep></entry>
      <entry><ind>0.700000</ind><dep>0.400000</dep></entry>
      <entry><ind>0.600000</ind><dep>0.000000</dep></entry>
      <entry><ind>0.500000</ind><dep>0.000000</dep></entry>
      <entry><ind>0.400000</ind><dep>0.000000</dep></entry>
      <entry><ind>0.300000</ind><dep>0.000000</dep></entry>
      <entry><ind>0.200000</ind><dep>0.000000</dep></entry>
      <entry><ind>0.100000</ind><dep>0.000000</dep></entry>
      <entry><ind>0.050000</ind><dep>0.000000</dep></entry>
      <entry><ind>0.000000</ind><dep>0.000000</dep></entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
        <center>
            <x-m>6.99646</x-m>
            <y-m>5.61966</y-m>
            <z-m>-5.10007</z-m>
        </center>
  </animation>
		
    <!-- Pitch -->
    <animation>
        <type>rotate</type>
        <object-name>shadow</object-name>
        <property>sim/multiplay/generic/float[8]</property>
        <factor>-1.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    

    <animation>
        <type>translate</type>
        <object-name>shadow</object-name>
        <property>sim/multiplay/generic/float[7]</property>
        <factor>0.3048</factor>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>shadow</object-name>
        <condition>
         <and>
         <not>
            <property>/sim/rendering/shadows/enabled</property>
          </not>
          <less-than>
            <property>sim/multiplay/generic/float[7]</property>
            <value>100</value>
          </less-than>
          <greater-than>
            <property>sim/multiplay/generic/float[7]</property>
            <value>0</value>
          </greater-than>
         </and>
        </condition>
    </animation>
    
    <!-- Pitch -->
    <animation>
        <type>rotate</type>
        <object-name>shadow1</object-name>
        <property>sim/multiplay/generic/float[8]</property>
        <factor>-1.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

<!-- roll -->
    <animation>
        <type>rotate</type>
        <object-name>shadow1</object-name>
        <property>sim/multiplay/generic/float[9]</property>
        <factor>1.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    
    <animation>
        <type>translate</type>
        <object-name>shadow1</object-name>
        <property>sim/multiplay/generic/float[7]</property>
        <factor>0.3048</factor>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>shadow1</object-name>
        <condition>
         <and>
         <not>
            <property>/sim/rendering/shadows/enabled</property>
          </not>
          <less-than>
            <property>sim/multiplay/generic/float[7]</property>
            <value>6500</value>
          </less-than>
          <greater-than>
            <property>sim/multiplay/generic/float[7]</property>
            <value>100</value>
          </greater-than>
         </and>
        </condition>
    </animation>        
   
</PropertyList>	
