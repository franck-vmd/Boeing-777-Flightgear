<animation>
	<type>translate</type>
	<object>cater_bridge3</object>
	<object>cater_truck3</object>
	<object>gourmet_cargo3</object>
	<object>gourmet_railing3</object>
	<object>gourmet_scissor_inner3</object>
	<object>gourmet_scissor_outer3</object>
	<property>gear/gear[2]/compression-norm</property>
	<interpolation>
		<entry><ind>0.15</ind><dep>0.0</dep></entry>
		<entry><ind>0.40</ind><dep>0.15</dep></entry>
	</interpolation>
	<axis>
		<x>0</x>
		<y>0</y>
		<z>1</z>
	</axis>
</animation>

</PropertyList>
