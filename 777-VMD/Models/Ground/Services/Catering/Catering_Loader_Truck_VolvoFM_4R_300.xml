<?xml version="1.0"?>
<PropertyList>

	<!-- catering truck implemented on the 777 by franck.VMD -->
	<path>Catering_Loader_Truck_VolvoFM_4R.ac</path>
	
	# Visible or not
	
	<animation>
		<type>select</type>
		<object>cater_bridge3</object>
		<object>cater_truck3</object>
		<object>gourmet_cargo3</object>
		<object>gourmet_railing3</object>
		<object>gourmet_scissor_inner3</object>
		<object>gourmet_scissor_outer3</object>
		<condition>
			<and>
				<equals>
					<property>services/catering/enable3</property>
					<value>1</value>
				</equals>
				<less-than>
						<property>velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</and>
		</condition>
	</animation>
	
	# Pick animations

	<animation>
        <type>pick</type>
		<object-name>cater_bridge3</object-name>
		<object-name>gourmet_cargo3</object-name>
		<object-name>gourmet_railing3</object-name>
		<object-name>gourmet_scissor_inner3</object-name>
		<object-name>gourmet_scissor_outer3</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-toggle</command>
                <property>services/catering/move3</property>
            </binding>
        </action>
    </animation>
	
	<animation>
        <type>pick</type>
		<object-name>cater_truck3</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-toggle</command>
                <property>services/catering/enable3</property>
            </binding>
        </action>
    </animation>	
	
	# Positioning
	
	<animation>
		<type>translate</type>
		<object>cater_bridge3</object>
		<object>cater_truck3</object>
		<object>gourmet_cargo3</object>
		<object>gourmet_railing3</object>
		<object>gourmet_scissor_inner3</object>
		<object>gourmet_scissor_outer3</object>
		<property>sim/model/ramp/x-m</property>
		<offset-m>19.284</offset-m>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	# Elevator Animation

	<animation>
		<type>translate</type>
		<object-name>cater_bridge3</object-name>
		<property>services/catering/position3</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0.0</dep>
			</entry>
			<entry>
				<ind>0.47368421052</ind>
				<dep>0.0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>2.0</dep>
			</entry>
		</interpolation> 
		<axis>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>gourmet_railing3</object-name>
		<object-name>gourmet_cargo3</object-name>
		<property>services/catering/position3</property>
		<factor>3.8</factor>
		<axis>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>gourmet_scissor_outer3</object-name>
		<object-name>gourmet_scissor_inner3</object-name>
		<property>services/catering/position3</property>
		<factor>1.9</factor>
		<axis>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>gourmet_scissor_outer3</object-name>
		<property>services/catering/position3</property>
		<factor>32</factor>
		<expression>
		    <rad2deg>
				<asin>
					<div>
						<product>
							<property>services/catering/position3</property>
							<value>3.8</value>
						</product>
						<value>7.36481</value>
					</div>
				</asin>
			</rad2deg>
		</expression>
		<center>
			<x-m>35.22785</x-m>
			<y-m>8.59021</y-m>
			<z-m>-4.46443</z-m>
		</center>
		<axis>
		    <x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
