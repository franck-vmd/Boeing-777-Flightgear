<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>flight_attendants_x9.ac</path>

  <animation>
		<type>material</type>
		<object-name>Flight_attend.x9</object-name>
		<texture>ensemble.png</texture>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>galleycartx9</object-name>
		<texture>galley-cart.png</texture>
	</animation>

        <animation>
        <type>material</type>
                         <object-name>Flight_attend.x9</object-name>
                         <object-name>galleycartx9</object-name>
        <condition>
         <and>
		<greater-than>
			<property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
		</greater-than>
        <greater-than>
			<property>systems/electrical/outputs/cabin-lights</property>
                <value>1.0</value>
		</greater-than>
         </and>
        </condition>
        <emission>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </emission>
    </animation>
 
</PropertyList>
