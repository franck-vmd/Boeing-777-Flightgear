<animation>
	<type>select</type>
	<object-name>fuel_pipe</object-name>
	<condition>
		<equals>
			<property>services/fuel-truck/connect</property>
			<value>1</value>
		</equals>
	</condition>
</animation>

<animation>
	<type>select</type>
	<object-name>fuel_pipe_disconnected</object-name>
	<condition>
		<equals>
			<property>services/fuel-truck/connect</property>
			<value>0</value>
		</equals>
	</condition>
</animation>

<animation>
	<type>translate</type>
	<object>fuel_pipe_disconnected</object>
	<object>fuel_truck</object>
	<object>handle</object>
	<property>gear/gear[1]/compression-norm</property>
	<interpolation>
		<entry><ind>0.15</ind><dep>0.0</dep></entry>
		<entry><ind>0.40</ind><dep>0.25</dep></entry>
	</interpolation>
	<axis>
		<x>0</x>
		<y>0</y>
		<z>1</z>
	</axis>
</animation>

<animation>
	<type>pick</type>
	<object-name>fuel_truck</object-name>
	<action>
		<button>0</button>
		<binding>
			<command>property-toggle</command>
			<property>services/fuel-truck/enable</property>
		</binding>
	</action>
</animation>

<animation>
	<type>pick</type>
	<object-name>fuel_pipe</object-name>
	<action>
		<button>0</button>
		<binding>
			<command>property-toggle</command>
			<property>services/fuel-truck/connect</property>
		</binding>
	</action>
</animation>

<animation>
	<type>pick</type>
	<object-name>fuel_pipe_disconnected</object-name>
	<action>
		<button>0</button>
		<binding>
			<command>property-toggle</command>
			<property>services/fuel-truck/connect</property>
		</binding>
	</action>
</animation>

</PropertyList>
