<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>

	<path>chock_left.ac</path>
	
<animation>
	<type>translate</type>
	<object>Chock_front</object>
        <object>Chock_back</object>
	<property>gear/gear[1]/compression-norm</property>
	<interpolation>
		<entry><ind>0.15</ind><dep>0.0</dep></entry>
		<entry><ind>0.40</ind><dep>0.25</dep></entry>
	</interpolation>
	<axis>
		<x>0</x>
		<y>0</y>
		<z>1</z>
	</axis>
</animation>

	<animation>
        <type>pick</type>
		<object-name>Chock_front</object-name>
                <object-name>Chock_back</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-toggle</command>
                <property>services/chocks/left</property>
            </binding>
        </action>
    </animation>

</PropertyList>
