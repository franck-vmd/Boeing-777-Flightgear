<?xml version="1.0"?>

<PropertyList>

  <path>evacuation2.ac</path>

    <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
  </animation>

  
   <animation>
      <type>material</type>
    <object-name>Evacuation_trap.1</object-name>
      <condition>
         <greater-than>
            <property>/sim/time/sun-angle-rad</property>
            <value>1.59</value>
         </greater-than>
      </condition>
      <global type="bool">true</global>
      <emission>
         <red>0.3</red>
         <green>0.3</green>
         <blue>0.3</blue>
      </emission>
   </animation>
   
   <animation>
      <type>material</type>
    <object-name>Evacuation_trap.1</object-name>
      <condition>
         <less-than-equals>
            <property>/sim/time/sun-angle-rad</property>
            <value>1.59</value>
         </less-than-equals>
      </condition>
      <global type="bool">true</global>
      <emission>
         <red>0</red>
         <green>0</green>
         <blue>0</blue>
      </emission>
   </animation>
  
</PropertyList>
  
