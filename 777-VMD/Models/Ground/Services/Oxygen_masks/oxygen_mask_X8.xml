<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>oxygen_mask_X8.ac</path>

 <animation>
		<type>material</type>
		<object-name>Oxy_mask_1</object-name>
		<condition>
		<greater-than-equals>
				<property>/systems/electrical/outputs/efis</property>
				<value>15</value>
		</greater-than-equals>
		</condition>
	</animation>
	
	<animation>
        <type>material</type>
         <object-name>Oxy_mask_1.001</object-name>
        <condition>
         <and>
		<greater-than>
			<property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
		</greater-than>
        <greater-than>
			<property>systems/electrical/outputs/cabin-lights</property>
                <value>1.0</value>
		</greater-than>
         </and>
        </condition>
        <emission>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </emission>
    </animation>
  
</PropertyList>
