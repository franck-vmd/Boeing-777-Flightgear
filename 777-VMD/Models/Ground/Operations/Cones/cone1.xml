<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

<path>Cone1.ac</path>

<animation>
    <type>select</type>
    <object-name>Cone1</object-name>
    <condition>
        <equals>
            <property>services/cones/cone1-enable</property>
            <value>1</value>
        </equals>
    </condition>
</animation>

<animation>
    <type>translate</type>
    <object-name>Cone1</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
        <entry><ind>0.18</ind><dep>0</dep></entry>
        <entry><ind>0.21</ind><dep>0.03</dep></entry>
    </interpolation>
    <axis>
        <x>0</x>
        <y>0</y>
        <z>1</z>
    </axis>
</animation>
  
<animation>
    <type>pick</type>
    <object-name>Cone1</object-name>
    <action>
        <button>0</button>
        <binding>
            <command>property-toggle</command>
            <property>services/cones/cone1-enable</property>
        </binding>
    </action>
</animation>

</PropertyList>
