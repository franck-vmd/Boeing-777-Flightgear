<!-- OSG hotspots -->

    <!-- G/S INHIBIT button -->
    <animation>
        <type>pick</type>
        <object-name>GS_INH_Switch</object-name>
        <object-name>GS_INH_GND</object-name>
        <object-name>GS_INH_Ind</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>
                  controls.click(3);
                  b777.Gpws.clicked_gs_override();
                  b777.Gpws.press_selftest_button();
                </script>
            </binding>
            <mod-up>
              <binding>
                <command>nasal</command>
                <script>
                  b777.Gpws.release_selftest_button();
                </script>
              </binding>
            </mod-up>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>GS_INH_Switch</object-name>
        <object-name>GS_INH_GND</object-name>
        <object-name>GS_INH_Ind</object-name>
        <action>
            <button>1</button> <!-- middle button to simulate long push-button press... -->
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>
                  controls.click(4);
                  b777.Gpws.clicked_selftest();
                </script>
            </binding>
        </action>
    </animation>
    
    <animation>
   <type>translate</type>
   <object-name>GS_INH_Switch</object-name>
    <property>instrumentation/mk-viii/outputs/discretes/gpws-warning</property>
   <factor>0.004</factor>
   <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
   </axis>
 </animation>

