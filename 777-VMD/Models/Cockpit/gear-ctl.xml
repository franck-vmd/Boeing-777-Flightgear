    <animation>
        <type>pick</type>
        <object-name>GearLever</object-name>
        <object-name>GearLever.001</object-name>
        <action>
            <button>1</button>
            <binding>
                <command>nasal</command>
                <script>controls.gearDown(-1)</script>
            </binding>
            <mod-up>
                <binding>
                    <command>nasal</command>
                    <script>controls.gearDown(0)</script>
                </binding>
            </mod-up>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>GearLever</object-name>
        <object-name>GearLever.001</object-name>
        <action>
            <button>0</button>
