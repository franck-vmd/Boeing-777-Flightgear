<PropertyList>

    <path>gear-ctl.ac</path>

    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>Lettering_Gear</object-name>
        <object-name>GearLever.001</object-name>
        <object-name>TERRAIN_OVRD_TranspCover</object-name>
        <object-name>FLAP_OVRD_TranspCover</object-name>
        <object-name>GEAR_OVRD_TranspCover</object-name>
    </effect>

    <effect>
        <name>777-interior</name>
        <inherits-from>Aircraft/777-VMD/Models/Effects/777-interior</inherits-from>
        <object-name>gearCTL</object-name>
        <object-name>BlockAltGr_Cover</object-name>
        <object-name>AltGr.knob</object-name>
        <object-name>BlockAltGr_Flange</object-name>
        <object-name>BlockAltGr_Pivot</object-name>
        <object-name>GearLever</object-name>
        <object-name>GearLever.001</object-name>
        <object-name>Lock.OVR</object-name>
    </effect>

    <animation>
        <type>material</type>
        <object-name>Lettering_Gear</object-name>
        <condition>
            <greater-than>   
                <property>/sim/time/sun-angle-rad</property>
                <value>1.70</value>
            </greater-than>
             <greater-than>
                <property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
             </greater-than>
        </condition>
        <emission>
            <red>0.2</red>
            <green>0.8</green>
            <blue>0.2</blue>
            <factor-prop>controls/lighting/panel-flood-intencity</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>AutoBrake.knob</object-name>
        <object-name>AutoBrake.001</object-name>
        <property>autopilot/autobrake/step</property>
        <interpolation>
            <entry>
                <ind>-2</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>-1</ind>
                <dep>59</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>91</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>120.0</dep>
            </entry>
            <entry>
                <ind>2</ind>
                <dep>150.0</dep>
            </entry>
            <entry>
                <ind>3</ind>
                <dep>180.0</dep>
            </entry>
            <entry>
                <ind>4</ind>
                <dep>206.0</dep>
            </entry>
            <entry>
                <ind>5</ind>
                <dep>237.0</dep>
            </entry>
        </interpolation>
        <axis>
        <x1-m>-1.80926</x1-m>
        <y1-m>0.148742</y1-m>
        <z1-m>0.259852</z1-m>
        <x2-m>-1.85394</x2-m>
        <y2-m>0.148738</y2-m>
        <z2-m>0.251972</z2-m>
        </axis>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>GearLever</object-name>
        <object-name>GearLever.001</object-name>
        <property>controls/gear/gear-down</property>
        <factor>30</factor>
        <center>
            <x-m>-1.838</x-m>
            <y-m>0.135675</y-m>
            <z-m>0.385275</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>select</type>
        <object-name>GS_INH_GND</object-name>
        <condition>
            <property>instrumentation/mk-viii/inputs/discretes/glideslope-inhibit</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>GS_INH_Ind</object-name>
        <condition>
            <property>instrumentation/mk-viii/outputs/discretes/gpws-warning</property>
        </condition>
    </animation>

 <animation>
   <type>translate</type>
   <object-name>FLAP_OVRD_AcryFace</object-name>
   <object-name>FLAP_OVRD_Switch</object-name>
   <object-name>FLAP_OVRD_Shutter.Top</object-name>
   <object-name>FLAP_OVRD_Shutter.Bottom</object-name>
