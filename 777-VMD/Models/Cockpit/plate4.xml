Franck.VMD
-->

<PropertyList>

    <path>plate4.ac</path>
    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>plate4</object-name>
        <object-name>ShipIDR</object-name>
        <object-name>CompanyIDR</object-name>
    </effect>

    <animation>
        <type>material</type>
        <object-name>plate4</object-name>
        <object-name>ShipIDL</object-name>
        <object-name>ShipIDR</object-name>
        <object-name>CompanyIDL</object-name>
        <object-name>CompanyIDR</object-name>
        <condition>
            <greater-than>
                <property>/sim/time/sun-angle-rad</property>
                <value>1.70</value>
            </greater-than>
             <greater-than>
                <property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
             </greater-than>
        </condition>
        <emission>
            <red>0.2</red>
            <green>0.8</green>
            <blue>0.2</blue>
            <factor-prop>controls/lighting/panel-flood-intencity</factor-prop>
        </emission>
    </animation>
    <text>
        <name>ShipIDL</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>-0.0588</y-m>
            <z-m>-0.0054</z-m>
        </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type type="string">text-value</type>
        <property>sim/multiplay/callsign</property>
        <font>helvetica_bold.txf</font>
        <character-size>0.011</character-size>
        <character-aspect-ratio type="double">1.5</character-aspect-ratio>
        <font-resolution>
            <width type="int">64</width>
            <height type="int">64</height>
        </font-resolution>
    </text>

    <text>
        <name>CompanyIDL</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>-0.0588</y-m>
            <z-m>0.0086</z-m>
        </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type type="string">text-value</type>
        <property>sim/aircraft-operator</property>
        <font>helvetica_bold.txf</font>
        <character-size>0.011</character-size>
        <character-aspect-ratio type="double">1.5</character-aspect-ratio>
        <font-resolution>
            <width type="int">64</width>
            <height type="int">64</height>
        </font-resolution>
    </text>
     <text>
         <name>ShipIDR</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>0.816</y-m>
            <z-m>-0.0087</z-m>
        </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type type="string">text-value</type>
        <property>sim/multiplay/callsign</property>
        <font>helvetica_bold.txf</font>
        <character-size>0.011</character-size>
        <character-aspect-ratio type="double">1.5</character-aspect-ratio>
        <font-resolution>
            <width type="int">64</width>
            <height type="int">64</height>
        </font-resolution>
    </text>
    <text>
        <name>CompanyIDR</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>0.816</y-m>
            <z-m>0.0053</z-m>
        </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type type="string">text-value</type>
        <property>sim/aircraft-operator</property>
        <font>helvetica_bold.txf</font>
        <character-size>0.011</character-size>
        <character-aspect-ratio type="double">1.5</character-aspect-ratio>
        <font-resolution>
            <width type="int">64</width>
            <height type="int">64</height>
        </font-resolution>
    </text>

     <animation>
        <type>material</type><!--white-->
        <object-name>ShipIDL</object-name>
        <object-name>ShipIDR</object-name>
        <object-name>CompanyIDL</object-name>
        <object-name>CompanyIDR</object-name>
       <emission>
            <red>0.1</red>
            <green>0.1</green>
            <blue>0.1</blue>
            <factor-prop>controls/lighting/master-bright-intencity</factor-prop>
        </emission>
    </animation>
</PropertyList>
