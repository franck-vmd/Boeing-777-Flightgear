<PropertyList>

<path>boeing2.ac</path>


<animation>
    <type>material</type>
    <texture alias="../../texture"/>
</animation>

     <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>Lettering_Cdu</object-name>
        <object-name>Lettering_Alpha</object-name>
        <object-name>Lettering_Numbers</object-name>
        <object-name>Lettering_Commands</object-name>
        <object-name>Lettering_Btns</object-name>
    </effect>
     
     <animation>
   <type>textranslate</type> 
   <object-name>CDuscreen</object-name> 		
   <property>/sim/time/sun-angle-rad</property>
   <step>1.70</step>
   <factor>0.286</factor>
   <center>
     <x-m>0</x-m>
     <y-m>0</y-m>
     <z-m>0</z-m>
   </center>
   <axis> 
     <x>1</x> 
     <y>0</y> 
     <z>0</z> 
   </axis>
</animation>

<animation> 
 <type>material</type> 
 <object-name>CDuscreen</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.70</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.3</red>	
  <green>0.3</green>	
  <blue>0.3</blue>
 </emission>
</animation>

     <animation>
        <type>material</type>
        <object-name>CDU</object-name>
        <condition>
                <greater-than>
			<property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
		</greater-than>
                <property>controls/lighting/cockpit</property>
        </condition>
        <emission>
            <red>0.5</red>
            <green>0.5</green>
            <blue>0.5</blue>
            <factor-prop>controls/lighting/dome-intencity</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>Lettering_Cdu</object-name>
        <object-name>Lettering_Alpha</object-name>
        <object-name>Lettering_Numbers</object-name>
        <object-name>Lettering_Commands</object-name>
        <object-name>Lettering_Btns</object-name>
        <condition>
             <greater-than>
                <property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
             </greater-than>
             <greater-than>
                <property>/sim/time/sun-angle-rad</property>
                <value>1.70</value>
             </greater-than>
        </condition>
        <emission>
            <red>0.2</red>
            <green>0.8</green>
            <blue>0.2</blue>
            <factor-prop>controls/lighting/GLRSFlood-intencity</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
                <object-name>Lettering_Cdu</object-name>
                <object-name>Lettering_Alpha</object-name>
                <object-name>Lettering_Numbers</object-name>
                <object-name>Lettering_Commands</object-name>
                <object-name>Lettering_Btns</object-name>
        <condition>
		<greater-than>
			<property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
		</greater-than>
        </condition>
        <emission>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
            <factor-prop>controls/lighting/master-bright-intencity</factor-prop>
        </emission>
    </animation>

