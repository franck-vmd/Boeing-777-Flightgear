    <animation>
        <type>knob</type>
        <object-name>Tun1.knob</object-name>
        <object-name>Tun1.knob.001</object-name>
        <property>instrumentation/rmu/unit[1]/standby-mhz</property>
        <center>
            <x-m>0.007</x-m>
            <y-m>0.032</y-m>
            <z-m>-0.020</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
        <action>
            <binding>
                <condition>
                    <and>
                        <property>systems/electrical/outputs/avionics</property>
                        <property>instrumentation/rmu/unit[1]/vhf-l</property>
                    </and>
                </condition>
                <command>property-adjust</command>
                <property>instrumentation/comm[0]/frequencies/standby-channel</property>
                <factor>1</factor>
            </binding>
            <binding>
                <condition>
                    <and>
                        <property>systems/electrical/outputs/avionics</property>
                        <property>instrumentation/rmu/unit[1]/vhf-r</property>
                    </and>
                </condition>
                <command>property-adjust</command>
                <property>instrumentation/comm[1]/frequencies/standby-channel</property>
                <factor>1</factor>
            </binding>
            <binding>
                <condition>
                    <and>
                        <property>systems/electrical/outputs/avionics</property>
                        <property>instrumentation/rmu/unit[1]/vhf-c</property>
                    </and>
                </condition>
                <command>property-adjust</command>
                <property>instrumentation/comm[2]/frequencies/standby-channel</property>
                <factor>1</factor>
            </binding>
            <binding>
                <condition>
                    <and>
                        <property>systems/electrical/outputs/avionics</property>
                        <property>instrumentation/rmu/unit[1]/vhf-l</property>
                    </and>
                </condition>
                <command>property-assign</command>
                <property>instrumentation/rmu/unit[1]/standby-mhz</property>
                <property>instrumentation/comm[0]/frequencies/standby-mhz</property>
            </binding>
            <binding>
                <condition>
                    <and>
                        <property>systems/electrical/outputs/avionics</property>
                        <property>instrumentation/rmu/unit[1]/vhf-r</property>
                    </and>
                </condition>
                <command>property-assign</command>
                <property>instrumentation/rmu/unit[1]/standby-mhz</property>
                <property>instrumentation/comm[1]/frequencies/standby-mhz</property>
            </binding>
            <binding>
