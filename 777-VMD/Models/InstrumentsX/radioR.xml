<PropertyList>
    <path>radio.ac</path>

    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>freq</object-name>
        <object-name>freq.001</object-name>
        <object-name>freq.002</object-name>
        <object-name>freq.003</object-name>
        <object-name>freq.004</object-name>
        <object-name>freq.005</object-name>
        <object-name>freq.006</object-name>
        <object-name>stby</object-name>
        <object-name>stby.001</object-name>
        <object-name>stby.002</object-name>
        <object-name>stby.003</object-name>
        <object-name>stby.004</object-name>
        <object-name>stby.005</object-name>
        <object-name>stby.006</object-name>

    </effect>

    <animation>
        <!--name>comm1</name-->
        <type>select</type>
        <object-name>freq</object-name>
        <object-name>freq.001</object-name>
        <object-name>freq.002</object-name>
        <object-name>freq.003</object-name>
        <object-name>freq.004</object-name>
        <object-name>freq.005</object-name>
        <object-name>freq.006</object-name>
        <object-name>stby</object-name>
        <object-name>stby.001</object-name>
        <object-name>stby.002</object-name>
        <object-name>stby.003</object-name>
        <object-name>stby.004</object-name>
        <object-name>stby.005</object-name>
        <object-name>stby.006</object-name>
        <object-name>vhf-l.toggle</object-name>
        <object-name>vhf-r.toggle</object-name>
        <object-name>vhf-c.toggle</object-name>
        <object-name>hf-l.toggle</object-name>
        <object-name>hf-r.toggle</object-name>
        <condition>
            <greater-than>
			<property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
		</greater-than>
            <property>systems/electrical/outputs/avionics</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Offside</object-name>
        <condition>
            <and>
                <property>systems/electrical/outputs/avionics</property>
                <property>instrumentation/rmu/unit[1]/offside_tuned</property>
            </and>
        </condition>
    </animation>
    
    <animation>
        <type>material</type>
        <object-name>Tun2.knob.001</object-name>
        <object-name>Tun1.knob.001</object-name>
        <condition>
            <greater-than>
                <property>/sim/time/sun-angle-rad</property>
                <value>1.70</value>
            </greater-than>
             <greater-than>
                <property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
             </greater-than>
        </condition>
        <emission>
            <red>0.2</red>
            <green>0.8</green>
            <blue>0.2</blue>
            <factor-prop>controls/lighting/GLRSFlood-intencity</factor-prop>
        </emission>
    </animation>
    
    <animation>
        <type>material</type>
        <object-name>Tun2.knob</object-name>
        <object-name>Tun1.knob</object-name>
        <condition>
                <greater-than>
			<property>systems/electrical/CPT-FLT-INST</property>
                <value>24.0</value>
		</greater-than>
                <property>controls/lighting/cockpit</property>
        </condition>
        <emission>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
            <factor-prop>controls/lighting/dome-intencity</factor-prop>
        </emission>
    </animation>

<!-- Active freq nav mode -->

    <animation>
        <type>textranslate</type>
        <object-name>freq.001</object-name>
        <property>instrumentation/rmu/unit[1]/selected-mhz</property>
        <factor>0.001</factor>
        <step>100</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.002</object-name>
        <property>instrumentation/rmu/unit[1]/selected-mhz</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.003</object-name>
        <property>instrumentation/rmu/unit[1]/selected-mhz</property>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.004</object-name>
        <property>instrumentation/rmu/unit[1]/selected-mhz</property>
